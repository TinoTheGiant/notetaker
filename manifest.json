{
    "manifest_version": 3,
    "name": "Quick Note",
    "description": "A simple note-taking extension",
    "version": "1.0",
    "permissions": ["storage"],
    "action": {
      "default_popup": "/popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon32.png",
        "64": "icons/icon64.png",
        "128": "icons/icon128.png"
      }
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon32.png",
      "64": "icons/icon64.png",
      "128": "icons/icon128.png"
    }
  }